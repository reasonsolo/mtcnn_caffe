net : "landmark_train.prototxt"
#net : "train12.prototxt"
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.001
momentum: 0.9
weight_decay: 0.002
# The learning rate policy
lr_policy: "step"
stepsize: 5000
gamma: 0.8
display: 100
max_iter: 1000000
snapshot: 1000
snapshot_prefix: "./models/landmark"
solver_mode: GPU
