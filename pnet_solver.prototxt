net : "pnet_train.prototxt"
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.0001
momentum: 0.9
weight_decay: 0.004
# The learning rate policy
lr_policy: "step"
stepsize: 20000
gamma: 0.6
display: 1000   #500
max_iter: 300000
snapshot: 50000
snapshot_prefix: "./models/pnet"
solver_mode: GPU
