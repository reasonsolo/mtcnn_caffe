net : "pnet_train.prototxt"
#net : "train12.prototxt"
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.001
momentum: 0.9
weight_decay: 0.01
# The learning rate policy
lr_policy: "step"
stepsize: 30000
gamma: 0.7
display: 1000   #500
max_iter: 1000000
snapshot: 50000
snapshot_prefix: "./models/pnet"
solver_mode: GPU
